{
    "name": "gustavomorais/article",
    "version": "1.0.0",
    "description": "Article package",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GustavoMorais\\Article\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GustavoMorais\\Tests\\": "tests"
        }
    },    
    "authors": [
        {
            "name": "Gustavo Vinicius"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "GustavoMorais\\Article\\Providers\\ArticleProvider"
            ],
            "aliases": {
                "Location": "GustavoMorais\\Article\\Facades\\ArticleFacade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/routing": "^10.43",
        "illuminate/http": "^10.43",
        "webonyx/graphql-php": "^15.9",
        "php-amqplib/php-amqplib": "^3.0",
        "swiftmailer/swiftmailer": "^6.0"
    }
}
